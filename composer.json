{
    "name": "wamadev/nova-parental-field",
    "description": "A Laravel Nova field for single-table inheritance (Caleb Porzio's \"Parental\" package)",
    "keywords": [
        "laravel",
        "nova",
        "parental",
        "caleb porzio"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Wama\\NovaParentalField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wama\\NovaParentalField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
